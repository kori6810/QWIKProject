<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:orientation="vertical"    android:layout_width="match_parent"    android:layout_height="match_parent">    <View         android:id="@+id/back_to"        android:layout_width="15dp"         android:layout_height="15dp"         android:layout_marginStart="20dp"         android:layout_marginTop="60dp"         android:background="@drawable/ic_back"/>    <ImageView        android:layout_width="30dp"        android:layout_height="30dp"        android:src="@drawable/ic_camera"        android:layout_marginTop="168dp"        android:layout_marginStart="220dp"/>    <ImageView        android:id="@+id/set_profile"        android:layout_width="128dp"        android:layout_height="128dp"        android:src="@drawable/set_profile"        android:layout_centerHorizontal="true"        android:layout_marginTop="76dp"/>        <LinearLayout        android:layout_below="@id/set_profile"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical"        android:layout_marginHorizontal="30dp"        android:layout_marginTop="30dp">        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="Gender"            android:textSize="15sp"            android:textColor="#000000"            android:textStyle="bold|normal" />        <LinearLayout            android:layout_width="match_parent"            android:layout_height="50dp"            android:orientation="horizontal"            android:layout_marginTop="10dp">            <TextView                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:text="Male"                android:gravity="center"                android:textStyle="normal"                android:textColor="#000000"                android:textSize="15sp"                android:background="@drawable/profile_text_bc"/>            <TextView                android:layout_width="0dp"                android:layout_height="match_parent"                android:layout_weight="1"                android:text="Female"                android:gravity="center"                android:textStyle="normal"                android:textColor="@color/white"                android:textSize="15sp"                android:background="@drawable/profile_female_bc"/>        </LinearLayout>        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:text="Gender"            android:textSize="15sp"            android:textColor="#000000"            android:textStyle="bold|normal"            android:layout_marginTop="22dp"/>        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:text="77"            android:gravity="center_horizontal"            android:textSize="20sp"            android:textColor="@color/black"            android:fontFamily="@font/comfortaa"/>        <SeekBar            android:layout_width="match_parent"            android:layout_height="9dp"            android:max="100"            android:progress="77"            android:background="@drawable/progress_bar" />       <LinearLayout           android:layout_width="match_parent"           android:layout_height="wrap_content"           android:orientation="horizontal"           android:layout_marginTop="30dp">           <LinearLayout               android:layout_width="wrap_content"               android:layout_height="wrap_content"               android:orientation="vertical">               <TextView                   android:layout_width="wrap_content"                   android:layout_height="wrap_content"                   android:text="Height"                   android:textColor="#000000"                   android:textSize="15sp"                   android:textStyle="normal"/>               <EditText                   android:layout_width="135dp"                   android:layout_height="wrap_content"                   android:text="185 cm"                   android:textSize="20sp"                   android:textColor="@color/black"                   android:layout_marginTop="26dp"/>           </LinearLayout>           <LinearLayout               android:layout_width="wrap_content"               android:layout_height="wrap_content"               android:orientation="vertical"               android:layout_marginStart="40dp">               <TextView                   android:layout_width="wrap_content"                   android:layout_height="wrap_content"                   android:text="Height"                   android:textColor="#000000"                   android:textSize="15sp"                   android:textStyle="normal"/>               <EditText                   android:layout_width="135dp"                   android:layout_height="wrap_content"                   android:text="77 kg"                   android:textSize="20sp"                   android:textColor="@color/black"                   android:layout_marginTop="26dp"/>           </LinearLayout>       </LinearLayout>    </LinearLayout>    <androidx.appcompat.widget.AppCompatButton        android:id="@+id/register_btn2"        android:layout_width="match_parent"        android:layout_height="52dp"        android:layout_marginHorizontal="20dp"        android:background="@drawable/button_bc"        android:text="REGISTER"        android:textColor="@color/white"        android:textSize="13sp"        android:fontFamily="@font/roboto_bold"        android:layout_alignParentBottom="true"        android:layout_marginBottom="50dp"/></RelativeLayout>