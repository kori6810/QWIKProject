<?xml version="1.0" encoding="utf-8"?><layer-list xmlns:android="http://schemas.android.com/apk/res/android">    <item        android:id="@android:id/background">        <shape android:shape="ring"            android:thickness="30dp"            android:useLevel="false">            <corners android:radius="30dp"/>            <solid android:color="@color/white"/>        </shape>    </item>    <item        android:id="@android:id/progress">        <shape android:shape="ring"            android:thickness="30dp"            android:useLevel="true">            <corners android:radius="30dp"/>            <solid android:color="#14213D"/>        </shape>    </item></layer-list>